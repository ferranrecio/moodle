{{!
    @template core_course/format

    Displays the complete course format.

    No example given for now.
}}
<li id="section-{{num}}"
        class="section main main {{#onlysummary}} section-summary {{/onlysummary}} clearfix {{sectionstyle}}"
        role="region"
        aria-labelledby="sectionid-{{id}}-title"
        data-sectionid="{{num}}"
        data-sectionreturnid="{{sectionreturnid}}">
    <div class="left side">{{{leftcontent}}}</div>
    <div class="right side">{{{rightcontent}}}</div>
    <div class="content">
        {{{header}}}
        {{{availability}}}
        <div class="summary">{{{summary}}}</div>
        {{{activities}}}
        {{{cmlist}}}
        {{{cmcontrols}}}
    </div>
</li>
