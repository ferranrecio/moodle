{{!
    @template core_course/course_format

    Displays the complete course format.

    No example given for now.
}}
<h2 class="accesshide">{{{title}}}</h2>
{{{completionhelp}}}
<ul class="{{format}}">{{! The class should be part of the format output}}
    {{{initialsection}}}
    {{#sections}}
        {{{.}}}
    {{/sections}}
</ul>
{{#hasnavigation}}
<div class="single-section">
    {{{sectionnavigation}}}
    <ul class="{{format}}">{{! The class should be part of the format output}}
        {{{singlesection}}}
    </ul>
    {{{sectionselector}}}
</div>
{{/hasnavigation}}
{{{numsections}}}
